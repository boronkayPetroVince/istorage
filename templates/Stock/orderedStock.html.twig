    <script src="{{ asset('assets/bootstrap/js/jquery.min.js') }}"></script>
    <script>
        var rowId="";
        function updateItem(rowId){
            rowId = rowId.replace("btn","");
            console.log(rowId);
            id = document.getElementById("id"+rowId).innerHTML;
            capacity = document.getElementById("phoneCapacity"+rowId).innerHTML.replace("GB", "");
            document.getElementById("updateAmount").value = document.getElementById("amount"+rowId).innerHTML;
            document.getElementById("updatePrice").value = document.getElementById("purchasePrice"+rowId).innerHTML;
            document.getElementById("updateBrand").value = document.getElementById("brand"+rowId).innerHTML;
            document.getElementById("updateModel").value = document.getElementById("model"+rowId).innerHTML;
            document.getElementById("updateColor").value = document.getElementById("color"+rowId).innerHTML;
            document.getElementById("updateCapacity").value = parseInt(capacity);

            $("#updateStatus").html("");
            $("#updateClient").html("");
            $("#updateWarehouse").html("");
            $("#updateBrand").html("");
            $("#updateModel").prop('disabled', 'disabled');
            $("#updateColor").prop('disabled', 'disabled');
            $("#updateCapacity").prop('disabled', 'disabled');
            $("#save").prop('disabled', 'disabled');

            $.ajax({
                url: "{{ path('allWarehouse') }}",
                success: function (result){
                    $.each(result, function (key,value){
                        var warehouseOption = $('<option>', {value:value.id, text:value.whname});
                        $('#updateWarehouse').append(warehouseOption);
                    });
                    document.getElementById("updateWarehouse").value = document.getElementById("whID"+rowId).innerHTML;
                }
            });
            $.ajax({
                url: "{{ path('allStatus') }}",
                success: function (result){

                    $.each(result, function (key,value){
                        var statusOption = $('<option>',{ value: value.id, text: value.status});
                        $("#updateStatus").append(statusOption);
                    });
                    document.getElementById("updateStatus").value = document.getElementById("statusID"+rowId).innerHTML;
                    var url = document.getElementById("updateForm").action;
                    var newUrl = url.replace("id", parseInt(id));
                    document.getElementById("updateForm").action = newUrl;
                    $("#updateOrder").show();
                }
            });

            $.ajax({
                url:"{{ path('selectAllClients') }}",
                method: "post",
                success: function (result){
                    $.each(result, function (key,value){
                        var clientOption = $('<option>', {value:value.id, text:value.clientName});
                        $("#updateClient").append(clientOption);
                    });
                    document.getElementById("updateClient").value = document.getElementById("clientID"+rowId).innerHTML;
                }
            });
            $.ajax({
                url:"{{ path('allBrands') }}",
                method: "post",
                success: function (result){
                    $.each(result, function (key,value){
                        var brandOption2 = $('<option>', {value:value.id, text:value.brandName});
                        $("#updateBrand").append(brandOption2);
                    });
                    document.getElementById("updateBrand").value = document.getElementById("brandID"+rowId).innerHTML;
                    $.ajax({
                        url:"{{ path('allModelByBrand') }}",
                        method: "post",
                        data:{
                            "brandNameID": document.getElementById("updateBrand").value
                        },
                        success:function (result){
                            $.each(result, function (key,value){
                                var modelOption = $('<option>', {value:value.modelID.id, text:value.modelID.modelName});
                                $("#updateModel").append(modelOption);
                            });
                            $("#updateModel").prop('disabled', false);
                            document.getElementById("updateModel").value = document.getElementById("modelID"+rowId).innerHTML;
                            $.ajax({
                                url:"{{ path('allColorByModel') }}",
                                method: "post",
                                data:{
                                    "modelNameID": document.getElementById("updateModel").value
                                },
                                success:function (result){
                                    $.each(result, function (key,value){
                                        var colorOption = $('<option>', {value:value.colorID.id, text:value.colorID.phoneColor});
                                        $("#updateColor").append(colorOption);
                                    });
                                    $("#updateColor").prop('disabled', false);
                                    document.getElementById("updateColor").value = document.getElementById("colorID"+rowId).innerHTML;
                                    $.ajax({
                                        url:"{{ path('allCapacityByModel') }}",
                                        method: "post",
                                        data:{
                                            "modelNameID": document.getElementById("updateModel").value,
                                            "colorNameID": document.getElementById("updateColor").value
                                        },
                                        success:function (result){
                                            $.each(result, function (key,value){
                                                var capacityOption = $('<option>', {value:value.capacityID.id, text:value.capacityID.capacity});
                                                $("#updateCapacity").append(capacityOption);
                                            });
                                            document.getElementById("updateCapacity").value = document.getElementById("phoneCapacityID"+rowId).innerHTML;
                                            $("#updateCapacity").prop('disabled', false);
                                            $("#save").prop('disabled', false);
                                        }
                                    });
                                }
                            });
                        }
                    });
                }
            });
        }
        function updateBrandChange(){
