{% extends 'index.html.twig' %}

{# @var stocks \App\Entity\Stock[] #}
{% block content %}
    <div class="container-fluid">
        <div class="alert alert-{{ resultColor }}" role="alert">
            <span class="fw-bold">{{ resultMessage }}</span>
        </div>
        <div class="card shadow" style="background: rgba(255,255,255,0.5);margin-bottom: 5%;">
            <div class="card-header py-3" style="background: rgb(0,50,71);color: rgb(255,255,255);">
                <h3 class=" mb-4">Megrendelt eszközök</h3>
            </div>
            <div class="card-body" style="background: rgba(255,255,255,0.5);">
                <div class="row">
                    <div class="text-nowrap col-md-6">
                        <div class="input-group" >
                            <div class="input-group-prepend">
                                <button style="background: #e6f8fe;opacity: 1; border:solid black; color:black;" disabled><i class="fa fa-search"></i></button>
                            </div>
                            <input type="text" id="myInput" onkeyup="searchFilter()" title="Telefon keresése">
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="text-md-end dataTables_filter" id="dataTable_filter">
                            <a href="{{ path('generateOrderedExcel') }}" class="btn btn-primary buttonDesign" type="button">Excel</a>
                            <a href="{{ path('generateOrderedPDF') }}" class="btn btn-primary buttonDesign" type="button">PDF</a>
                            <button onclick="addingStock()" class="btn btn-primary buttonDesign" type="button" data-bs-target="#addOrder" data-bs-toggle="modal">Hozzáadás</button>
                        </div>
                    </div>
                </div>
                <div class="scrollIt table-responsive table mt-2" id="dataTableDiv" role="grid" >
                    <table class="table table-striped my-0" id="dataTable">
                        <thead>
                        <tr>
                            <th>ID</th>
                            <th>Mennyiség</th>
                            <th>Beszerzési ár</th>
                            <th>Telefon</th>
                            <th>Raktár</th>
                            <th>Megrendelve</th>
                            <th>Partner</th>
                            <th>Felhasználó</th>
                            <th>Műveletek</th>
                        </tr>
                        </thead>
                        <tbody>
                        {% set counter = 0 %}
                        {% for stock in stocks|sort((a, b) => a.date <=> a.date) %}
                            {% if stock.statusID.status == "Megrendelve" %}
                                <tr id="{{ counter }}">
                                    <td id="id{{ counter }}">{{ stock.id }}</td>
                                    <td id="amount{{ counter }}">{{ stock.amount }}</td>
                                    <td id="purchasePrice{{ counter }}">{{ stock.purchasePrice }}</td>
                                    <td>
                                        <span id="brand{{ counter }}">{{ stock.phoneID.brandID.brandName }} </span>
                                            <span class="d-none" id="brandID{{ counter }}">{{ stock.phoneID.brandID.id }}</span>
                                        <span id="model{{ counter }}">{{ stock.phoneID.modelID.modelName }} </span>
                                            <span class="d-none" id="modelID{{ counter }}">{{ stock.phoneID.modelID.id }}</span>
                                        <span id="color{{ counter }}">{{ stock.phoneID.colorID.phoneColor }} </span>
                                            <span class="d-none" id="colorID{{ counter }}">{{ stock.phoneID.colorID.id }}</span>
                                        <span id="phoneCapacity{{ counter }}">{{ stock.phoneID.capacityID.capacity }} GB </span>
                                            <span class="d-none" id="phoneCapacityID{{ counter }}">{{ stock.phoneID.capacityID.id }}</span>
                                    </td>
                                    <td id="warehouse{{ counter }}">
                                        {{ stock.warehouseID.whName }}
                                        <span class="d-none" id="whID{{ counter }}">{{ stock.warehouseID.id }}</span>
                                    </td>
                                    <span class="d-none" id="statusID{{ counter }}">{{ stock.statusID.id }}</span>
                                    <td id="date{{ counter }}">{{ stock.date|date('Y-m-d H:i:s') }}</td>
                                    <td id="client{{ counter }}">
                                        {{ stock.clientID.clientName }}
                                        <span class="d-none" id="clientID{{ counter }}">{{ stock.clientID.id }}</span>
                                    </td>
                                    <td id="user{{ counter }}">{{ stock.userID.username }} ({{ stock.userID.roles[0] }})</td>
                                    <td>
                                        <button id="btn{{ counter }}" class="btn btn-primary buttonDesign" type="button" data-bs-target="#updateOrder" data-bs-toggle="modal" onclick="updateItem(this.id)"><i class="fas fa-edit operationIcon"></i></button>
                                    </td>
                                </tr>
                            {% endif %}
                            {% set counter = counter + 1 %}
                        {% endfor %}
                        </tbody>
                        <tfoot>
                        <tr></tr>
                        </tfoot>
                    </table>
                </div>
            </div>
        </div>
    </div>
    </div>
    <footer class="bg-white sticky-footer" style="background: #1e2833 !important;"></footer>
    </div><a class="border rounded d-inline scroll-to-top" href="#page-top"><i class="fas fa-angle-up"></i></a>
    </div>
    <div class="modal fade" role="dialog" tabindex="-1" id="updateOrder">
        <div class="modal-dialog modal-lg modal-fullscreen-lg-down" role="document">
            <div class="modal-content">
                <div class="modal-header modalHeaderFooter">
                    <h5 class="w-100 text-center"><strong>Megrendelt eszköz módosítása</strong><br></h5><button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <form id="updateForm" action="{{ path('updateOrderedStock', {"stockId": "id"}) }}" method="post">
                    <div class="modal-body modalBody">
                        <div class="card shadow mb-3" style="background: rgba(255,255,255,0.5);">
                            <div class="card-body" style="background: rgba(255,255,255,0.5);">
                                    <div class="row">
                                        <div class="col-12">
                                            <small style="color: red">Kérem várjon amíg az adatok betöltődnek!</small>
                                        </div>
                                        <div class="col col-12">
                                            <label class="form-label" for="updateClient"><strong>Partner</strong></label>
                                            <div class="input-group mb-3">
                                                <select name="updateClient" id="updateClient" class="form-select orderSelect" required></select>
                                                <div class="input-group-append">
                                                    <button style="border-radius: 0 !important;"  class="btn buttonDesign" data-bs-target="#addClient" data-bs-toggle="modal">+</button>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col col-sm-6 col-12">
                                            <label class="form-label" for="updateBrand"><strong>Márka</strong></label>
                                            <div class="input-group mb-3">
                                                <select name="updateBrand" id="updateBrand" class="form-select orderSelect" onchange="updateBrandChange()" required></select>
                                                <div class="input-group-append">
                                                    <button style="border-radius: 0 !important;" class="btn buttonDesign" data-bs-target="#addPhone" data-bs-toggle="modal">+</button>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col col-sm-6 col-12">
                                            <div class="mb-3">
                                                <label class="form-label" for="updateModel"><strong>Model</strong><br></label>
                                                <select name="updateModel" id="updateModel" class="form-select orderSelect" onchange="updateModellChange()" required>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col col-sm-6 col-12">
                                            <div class="mb-3">
                                                <label class="form-label" for="updateColor"><strong>Szín</strong><br></label>
                                                <select name="updateColor" id="updateColor" class="form-select orderSelect" onchange="updateColorChange()" required></select>
                                            </div>
                                        </div>
                                        <div class="col col-sm-6 col-12">
                                            <div class="mb-3">
                                                <label class="form-label" for="updateCapacity"><strong>Kapacitás</strong><br></label>
                                                <select name="updateCapacity" id="updateCapacity" class="form-select orderSelect" required></select>
                                            </div>
                                        </div>
                                        <div class="col-12">
                                            <label class="form-label" for="updateWarehouse"><strong>Raktár</strong><br></label>
                                            <select name="updateWarehouse" id="updateWarehouse" class="form-select orderSelect"></select>
                                        </div>
                                        <div class="col col-sm-6 col-12">
                                            <div class="mb-3">
                                                <label class="form-label" for="updateAmount"><strong>Mennyiség</strong><br></label>
                                                <input class="form-control" type="number" id="updateAmount" name="updateAmount">
                                            </div>
                                        </div>
                                        <div class="col col-sm-6 col-12">
                                            <div class="mb-3">
                                                <label class="form-label" for="updatePrice"><strong>Beszerzési ár</strong><br></label>
                                                <input class="form-control" type="number" id="updatePrice" name="updatePrice">
                                            </div>
                                        </div>
                                        <div class="col">
                                            <div class="mb-3"><label class="form-label" for="updateStatus"><strong>Státusz</strong><br></label>
                                                <select class="form-select orderSelect" name="updateStatus" id="updateStatus"></select>
                                            </div>
                                        </div>
                                    </div>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer modalHeaderFooter">
                        <button class="btn btn-light modalCloseButton" type="button" data-bs-dismiss="modal">Bezárás</button>
                        <button id="save" class="btn btn-primary modalSaveButton" type="submit">Mentés</button>
                    </div>
                </form>

            </div>
        </div>
    </div>
